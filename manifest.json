{
    "manifest_version": 3,
    "type": "extension",
    "name": "自动刷屏",
    "slug": "auto_sender",
    "description": "全自动刷屏辅助",
    "version": "0.1.0",
    "thumbnail": "./icon.png",
    "author": {
        "name": "bcmray_crazy",
        "link": "https://github.com/bcmRayCrazy-coder"
    },
    "repository": {
        "repo": "LiteLoaderQQNT/LiteLoaderQQNT-Plugin-Template",
        "branch": "main",
        "use_release": {
            "tag": "latest",
            "name": "Plugin-Template.zip"
        }
    },
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    }
}
